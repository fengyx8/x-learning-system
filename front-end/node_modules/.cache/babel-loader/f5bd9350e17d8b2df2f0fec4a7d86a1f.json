A;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;;AAXA,iDAYA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,eAAA,CAZA;;AAAA;AAAA;AAAA;;AAAA;AAeA,oBAAA,KAAA,CAAA,IAAA,CAAA,WAAA,IAAA,KAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,CAAA,WAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA;AACA,oBAAA,IAAA,EAAA,OADA;AAEA,oBAAA,OAAA,EAAA;AAFA,mBAAA;AAIA,iBAPA,MAOA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA;AACA;;AAxBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA;AA7BA;AAVA,CAAA","sourcesContent":["<template>\r\n  <el-card class=\"box-card\">\r\n    <div slot=\"header\" class=\"clearfix\">\r\n      <span>修改密码</span>\r\n    </div>\r\n    <el-form ref=\"form\" :model=\"form\" label-width=\"100px\">\r\n      <el-form-item style=\"margin-top:30px;margin-left:50px\" label=\"旧密码\">\r\n        <el-input\r\n          style=\"width:300px\"\r\n          v-model=\"form.password\"\r\n          show-password\r\n          placeholder=\"请输入旧密码\"\r\n        ></el-input>\r\n      </el-form-item>\r\n      <el-form-item style=\"margin-top:30px;margin-left:50px\" label=\"新密码\">\r\n        <el-input\r\n          style=\"width:300px\"\r\n          v-model=\"form.newPassword\"\r\n          show-password\r\n          placeholder=\"请输入新密码\"\r\n        ></el-input>\r\n      </el-form-item>\r\n      <el-form-item style=\"margin-top:30px;margin-left:50px\" label=\"确认新密码\">\r\n        <el-input\r\n          style=\"width:300px\"\r\n          v-model=\"form.rePassword\"\r\n          show-password\r\n          placeholder=\"请确认新密码\"\r\n        ></el-input>\r\n      </el-form-item>\r\n      <el-form-item style=\"margin-top:50px;margin-left:160px\">\r\n        <el-button type=\"primary\" @click=\"cancel\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"changePassword\">确认修改</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        password: '',\r\n        newPassword: '',\r\n        rePassword: '',\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    cancel () {\r\n      this.$router.push('baseInformation')\r\n    },\r\n    async changePassword() {\r\n      if (\r\n        this.form.password.length != 0 && \r\n        this.form.newPassword.length != 0 &&\r\n        this.form.newPassword === this.form.rePassword\r\n      ) {\r\n        //发送确认旧密码准确，然后设置成新密码\r\n        let res = await this.$http.put('userOperation/password?newPw=' + this.form.newPassword + \"&oldPw=\" + this.form.password)\r\n        console.log('改密码',res.data);\r\n        if(res.data.code == 200) {\r\n          window.sessionStorage.clear();\r\n          this.$router.push('/login');\r\n          return this.$message.success('密码修改成功！请重新登录。')\r\n        }\r\n\r\n      } else if(this.form.newPassword != this.form.rePassword) {\r\n        this.form.newPassword = '';\r\n        this.form.rePassword = '';\r\n        this.$message({\r\n          type: 'error',\r\n          message: '两次密码不一致！',\r\n        });\r\n      } else {\r\n        this.$message.error('修改失败！')\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  width: 60%;\r\n  margin: 0px 20% auto 20%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/personal"}]}