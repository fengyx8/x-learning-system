mation',\r\n          query: {\r\n            name: this.accountId,\r\n          },\r\n        });\r\n      } else {\r\n        this.form.newPassword = '';\r\n        this.form.rePassword = '';\r\n        this.$message({\r\n          type: 'error',\r\n          message: '新旧密码不一样！',\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.box-card {\r\n  width: 60%;\r\n  margin: 0px 20% auto 20%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/admin"}]}