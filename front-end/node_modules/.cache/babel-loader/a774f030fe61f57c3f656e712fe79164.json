   <el-button type=\"primary\" @click=\"cancel\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"add\">确定</el-button>\r\n      </el-form-item>\r\n    </el-form> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      fileList: [],\r\n      form: {\r\n        student: '',\r\n        name: '',\r\n        pwd: '',\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    async fileUpload (event) {\r\n      var file = document.getElementById(\"input\").files[0]; \r\n      console.log('文件',file);\r\n      var params = new FormData();\r\n      params.append('xUsersTable', file)\r\n      console.log(params.get(\"xUsersTable\"));\r\n      console.log('event',event)\r\n      let res = await this.$http.post('managerOperation/xUsers/' + params)\r\n      console.log('res',res)\r\n      \r\n    },\r\n    async add () {\r\n      if ( this.form.student && this.form.name && this.form.pwd ) {\r\n        let res = await this.$http.post(\"managerOperation/xUser?name=\" + this.form.name + \"&password=\" + this.form.pwd + \"&userId=\" + this.form.student);\r\n        console.log('add',res)\r\n        if(res.status == 200) {\r\n          this.$message.success('添加成功！');\r\n          this.$router.push('/admin/baseInformation');\r\n        } else {\r\n          this.$message.error('添加失败！')\r\n        }\r\n      } else {\r\n        this.$message({\r\n          type: 'error',\r\n          message: '添加失败！',\r\n        });\r\n      }\r\n    },\r\n    cancel () {\r\n      this.$router.push('/admin/baseInformation');\r\n    },\r\n    // 以下是文件上传相关的方法\r\n      submitUpload() {\r\n        console.log(this.fileList)\r\n        this.$refs.upload.submit();\r\n      },\r\n      handleRemove(file, fileList) {\r\n        console.log(file, fileList);\r\n      },\r\n      handlePreview(file) {\r\n        console.log(file);\r\n      }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n  .not-first-line {\r\n    margin-left: 10px\r\n  }\r\n  .btn-line {\r\n    margin-top: 30px;\r\n    margin-left: 60px\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/admin"}]}